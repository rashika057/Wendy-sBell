        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".MainMenuActivity" />
        <activity android:name=".ProductEntryActivity" />
        <activity android:name=".OrdersActivity" />
        <activity android:name=".OrderDetails" />
        <activity android:name=".TcActivity" />
        <activity android:name=".TeamActivity" />
        <activity android:name=".InfoActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EmployeeEntryActivity" />
        <activity android:name=".HelpActivity" />
        <activity android:name=".pop"
            android:theme="@style/AppTheme.CustomTheme">
